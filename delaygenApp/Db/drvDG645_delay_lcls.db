record(ai,"$(P)$(R)$(CHAN)Delay")
{
    field(PINI, "YES")
    field(DESC, "Chan $(CHAN) Delay")
    field(SCAN, "$(SCAN=1 second)")
    field(PREC, "12")
    field(EGU,  "Sec")
    field(HSV,  "$(HSV=MINOR)")
    field(LSV,  "$(LSV=MINOR)")
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn($(PORT)) $(CHAN)_DELAY")
    info(autosaveFields, "VAL DESC")
}
